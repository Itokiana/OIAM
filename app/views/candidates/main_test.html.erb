<link rel="stylesheet" type="text/css" href="/css/main_test.css">

<div class="all-content">
	<div class="left-link">
		<%= render partial: "candidates/left_link_bar" %>
	</div>
<div class="all-main">
	<div class="main">
		<h2 class="resultat-title">RÃ‰SULTATS DE MES TESTS</h2>
		<div class="resultats">
			<div class="potential">
				<div class="pontential-content">
					<p>Potential</p>
				</div>
				<div class="pontential-score">
					<p><%= @cadre.score_potential %></p>
				</div>
			</div>
			<div class="fit">
				<div class="fit-content">
					<p>Fit</p>
				</div>
