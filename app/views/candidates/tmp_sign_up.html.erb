<center><h2>INSCRIPTION</h2></center>

<%= form_for @cadreInfo , url: tmp_create_sign_up_path do |f| %>

  <div class="form-group">
    <%= f.text_field :last_name, autofocus: true, autocomplete: "NOM", required: true, placeholder:"NOM", class:"form-control" %>
  </div>

  <div class="form-group">
    <%= f.text_field :first_name, autofocus: true, autocomplete: "PRÉNOM", required: true, placeholder:"PRÉNOM", class:"form-control" %>
  </div>

  <div class="form-group">
    <%= f.text_field :adresse, autofocus: true, autocomplete: "ADRESSE", required: true, placeholder:"ADRESSE", class:"form-control" %>
  </div>
  <div class="row">
    <div class="col-sm-6 form-group">
      <%= f.text_field :postal_code, autofocus: true, autocomplete: "ADRESSE", required: true, placeholder:"CODE POSTAL", class:"form-control",id:"postal_code" %>
    </div>
    <div class="col-sm-6 form-group">
      <%= f.text_field :city, autofocus: true, autocomplete: "VILLE", required: true, placeholder:"VILLE", class:"form-control",id:"city" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.text_field :situation, autofocus: true, autocomplete: "SITUATION", required: true, placeholder:"SITUATION", class:"form-control" %>
  </div>

  <div class="form-group">
    <%= f.text_field :telephone, autofocus: true, autocomplete: "TÉLÉPHONE", required: true, placeholder:"TÉLÉPHONE", class:"form-control" %>
  </div>

  <div class="form-group">
    <%= f.email_field :mail, autofocus: true, autocomplete: "MAIL", required: true, placeholder:"MAIL", class:"form-control" %>
  </div>


  <div class="actions">
    <%= f.submit "S'INSCRIRE", class:"btn btn-primary" %>
  </div>

<% end %>


<script data-turbolinks-track="reload" type="text/javascript" src="/js/data.json"></script>

<script data-turbolinks-track="reload" type="text/javascript" src="/js/show_city.js"></script>


