  <div class="details-r">
    <%= f.text_field :last_name, autofocus: true, class:"dr-item", autocomplete: "last_name", required: true, placeholder: 'Nom' %> <%= f.text_field :first_name, autofocus: true, class:"dr-item", autocomplete: "first_name", required: true, placeholder: 'Prénom' %> <%= f.text_field :fonction,
    autofocus: true,class:"dr-item", autocomplete: "fonction", required: true, placeholder: 'Fonction' %> <%= f.email_field :mail, autofocus: true, class:"dr-item", autocomplete: "mail", required: true, placeholder: 'Mail' %> <%= f.text_field :telephone, autofocus: true, class:"dr-item",
    autocomplete: "telephone", required: true,placeholder: 'Téléphone' %>

    <div class="details-btn dr-btn2">
      SUIVANT
    </div>
  </div>

  <div class="details-r">
    <%= f.email_field :email, autofocus: true, class:"dr-item", autocomplete: "email", required: true, placeholder: 'IDENTIFIANT' %> <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, class:"dr-item", autocomplete: "new-password", required: true, placeholder: 'MOT DE PASSE' %> <%= f.password_field :password_confirmation, class:"dr-item", autocomplete: "new-password", required: true, placeholder: 'CONFIRMATION DU MOT DE PASSE' %>

