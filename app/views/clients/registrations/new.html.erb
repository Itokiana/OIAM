  </div>

  <div class="details-r">
    <input autofocus="autofocus" class="dr-item" required="required" autocomplete="entreprise_name" type="text" name="entreprise_name" id="entreprise_name" placeholder="Entreprise" />

    <input autofocus="autofocus" class="dr-item" required="required" autocomplete="entreprise_adresse" type="text" name="entreprise_adresse" id="entreprise_adresse" placeholder="Adresse" />

    <input autofocus="autofocus" class="dr-item dr-item-postal" required="required" autocomplete="postal_code" type="text" name="postal_code" id="postal_code" placeholder="Code Postal" />

    <input autofocus="autofocus" class="dr-item dr-item-fad" required="required" autocomplete="city" type="text" name="city" id="city" placeholder="Ville" readonly />

    <input autofocus="autofocus" class="dr-item" required="required" autocomplete="entreprise_siret" type="text" name="entreprise_siret" id="entreprise_siret" placeholder="Numéro de siret" />

    <input autofocus="autofocus" class="dr-item" required="required" autocomplete="entreprise_site" type="text" name="entreprise_site" id="entreprise_site" placeholder="Site Internet" />

    <textarea required="required" class="dr-item dr-textarea" autocomplete="entreprise_description" type="text" name="entreprise_description" id="entreprise_description" autofocus="autofocus" placeholder="Description de votre activité"></textarea>

    <input autofocus="autofocus" class="dr-item" required="required" autocomplete="code_naf" type="text" name="code_naf" id="code_naf" placeholder="Code NAF" />

    <div class="details-btn dr-btn1">
      SUIVANT
